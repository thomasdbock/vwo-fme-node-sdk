{
  "name": "vwo-fme-node-sdk",
  "version": "1.1.0",
  "description": "VWO Node SDK for Feature Management and Experimentation",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "check:versions": "node -e 'require(\"./scripts/check-versions\")'",
    "demo": "nodemon --inspect=0.0.0.0:9229 --legacy-watch --ignore node_modules demo/index.js",
    "demo:server": "nodemon --inspect=0.0.0.0:9229 --legacy-watch --ignore node_modules demo/server.js",
    "prettier": "prettier -w lib/**/*.ts *.md",
    "tsc": "yarn check:versions && tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wingify/vwo-fme-node-sdk"
  },
  "author": "VWO developers",
  "license": "Apache-2.0",
  "files": [
    "dist/",
    "package.json",
    "yarn.lock",
    "lib/**/*",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "NOTICE"
  ],
  "dependencies": {
    "murmurhash": "^2.0.1",
    "superstruct": "^1.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "express": "^4.19.2",
    "nodemon": "^2.0.6",
    "prettier": "^3.2.5",
    "semver": "^7.6.0",
    "shelljs": "^0.8.5",
    "typescript": "^5.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "globOptions": {
      "matchBase": false
    },
    "linters": {
      "**/*.js": [
        "yarn lint",
        "prettier",
        "git add"
      ],
      "**/*.{json,md}": [
        "prettier",
        "git add"
      ]
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  }
}
